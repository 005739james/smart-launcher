{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${workspaceRoot}/src/index.js",
                "cwd": "${workspaceRoot}",
                "env": {
                    "NODE_ENV": "development",
                    "LOG_TIMES": "1",
                    "DISABLE_SANDBOXES": "1",
                    "DISABLE_BACKEND_SERVICES": "0"
                }
            }
        ]
    }